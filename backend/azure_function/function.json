{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "{*route}",
      "methods": [ "get", "post", "put", "delete", "options" ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}